<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="lakion_sylius_elastic_search.controller.search" class="Lakion\SyliusElasticSearchBundle\Controller\SearchController">
            <argument type="service" id="fos_rest.view_handler" />
            <argument type="service" id="lakion_sylius_elastic_search.search.elastic_engine" />
            <argument type="service" id="form.factory" />
            <argument type="service" id="sylius.repository.taxon" />
            <argument type="service" id="sylius.context.shopper" />
        </service>
    </services>
</container>
